<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EasySeekBar">
        <attr name="ssb_bar_type" format="enum">
            <enum name="seekbar" value="0" />
            <enum name="diy" value="1" />
            <enum name="progress" value="2" />
            <enum name="lowOrHeight" value="3" />
        </attr>
        <attr name="ssb_bubble_type" format="enum">
            <enum name="circle" value="0" />
            <enum name="roundRect" value="1" />
        </attr>
        <attr name="ssb_text_show_type" format="enum">
            <enum name="up" value="0" />
            <enum name="down" value="1" />
            <enum name="inner" value="2" />
        </attr>

        <attr name="ssb_bar_height" format="dimension|reference" />
        <attr name="ssb_bar_image" format="reference" />
        <attr name="ssb_bar_color" format="color|reference" />
        <attr name="ssb_bar_stroke_width" format="dimension|reference" />
        <attr name="ssb_bar_stroke_color" format="color|reference" />
        <attr name="ssb_bar_progress_image" format="reference" />
        <attr name="ssb_bar_progress_color" format="color|reference" />

        <attr name="ssb_thumb_normal_radius" format="dimension|reference" />
        <attr name="ssb_thumb_select_radius" format="dimension|reference" />
        <attr name="ssb_thumb_width" format="dimension|reference" />
        <attr name="ssb_thumb_height" format="dimension|reference" />
        <attr name="ssb_thumb_image" format="reference" />
        <attr name="ssb_thumb_color" format="color|reference" />
        <attr name="ssb_thumb_select_color" format="color|reference" />
        <attr name="ssb_thumb_low_color" format="color|reference" />
        <attr name="ssb_thumb_height_color" format="color|reference" />

        <attr name="ssb_spacing" format="dimension|reference" />
        <attr name="ssb_text_size" format="dimension|reference" />
        <attr name="ssb_text_color" format="color|reference" />
        <attr name="ssb_text_select_color" format="color|reference" />
        <attr name="ssb_text_color_for_low" format="color|reference" />
        <attr name="ssb_text_color_for_height" format="color|reference" />

        <attr name="ssb_bubble_radius" format="dimension|reference" />
        <attr name="ssb_bubble_color" format="color|reference" />
        <attr name="ssb_bubble_text_size" format="dimension|reference" />
        <attr name="ssb_bubble_text_color" format="color|reference" />

        <attr name="ssb_max" format="integer" />
        <attr name="ssb_min" format="integer" />
        <attr name="ssb_progress" format="integer" />
        <attr name="ssb_low_progress" format="integer" />
        <attr name="ssb_height_progress" format="integer" />
        <attr name="ssb_diy_select_index" format="integer" />
        <attr name="ssb_animator_duration" format="integer" />

        <attr name="ssb_is_thumb_inner_offset" format="boolean" />
        <attr name="ssb_is_bar_round" format="boolean" />
        <attr name="ssb_is_show_text" format="boolean" />
        <attr name="ssb_is_show_float" format="boolean" />
        <attr name="ssb_is_show_bubble" format="boolean" />
        <attr name="ssb_is_can_touch" format="boolean" />
        <attr name="ssb_is_thumb_progress_part" format="boolean" />
        <attr name="ssb_is_diy_auto_fix" format="boolean" />
        <attr name="ssb_is_open_animator" format="boolean" />
        <attr name="ssb_is_thumb_no_over" format="boolean" />
        <attr name="ssb_is_thumb_no_over_than_move" format="boolean" />

    </declare-styleable>
</resources>